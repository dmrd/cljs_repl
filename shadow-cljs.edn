{:source-paths ["src"]
 :dependencies [
                [reagent "1.0.0-rc1"]
                [cider/cider-nrepl "0.25.5"]
]
 :nrepl {:port 9000}
 :dev-http {8081 "public"}
 :builds {
          :app {
                :target :browser
                :output-dir "public/js"
                :asset-path "/js"
                :modules {:main {:init-fn cljs_repl.core/main!}}
                :devtools {:http-root   "public" :http-port   3000}}
          }} 
